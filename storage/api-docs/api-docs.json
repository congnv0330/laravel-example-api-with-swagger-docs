{
    "openapi": "3.0.0",
    "info": {
        "title": "Laravel api",
        "description": "Example for laravel api",
        "version": "1.0"
    },
    "paths": {
        "/api/login": {
            "post": {
                "tags": [
                    "Authentication"
                ],
                "summary": "Login",
                "description": "Authenticate the request's credentials.",
                "operationId": "6a4b5a40514ca69a067072b272a1ef49",
                "requestBody": {
                    "description": "Parameters",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "email",
                                    "password"
                                ],
                                "properties": {
                                    "email": {
                                        "type": "string",
                                        "example": "example@gmail.com"
                                    },
                                    "password": {
                                        "type": "string",
                                        "example": "12345"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Login success",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "token": {
                                            "type": "string"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Validation failed"
                    }
                }
            }
        },
        "/api/blogs": {
            "get": {
                "tags": [
                    "Blog"
                ],
                "summary": "Get all blogs",
                "description": "Get all blogs",
                "operationId": "9819e5802278e060898795fe3c919599",
                "responses": {
                    "200": {
                        "description": "Get blog success"
                    }
                }
            }
        }
    },
    "security": [
        []
    ]
}